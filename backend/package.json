{
  "name": "tender-process-app",
  "version": "1.0.0",
  "description": "1. Create `.env` file by copying `.env.example` file and fill it with your environment variables\r 2. Install packages: `npm i`\r 3. Run the server: `npm start`",
  "main": "dist/index.js",
  "dependencies": {
    "@ethersproject/providers": "^5.7.2",
    "@orbitdb/liftoff": "^1.1.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "eciesjs": "^0.4.14",
    "express": "^4.21.2",
    "ipfs-http-client": "^60.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemon": "^3.1.7",
    "prisma": "^5.22.0",
    "uuid": "^11.1.0"
  },
  "scripts": {
    "start": "tsc && node dist/src/cmd/rest/index.js",
    "dev": "nodemon --exec ts-node ./cmd/index.ts",
